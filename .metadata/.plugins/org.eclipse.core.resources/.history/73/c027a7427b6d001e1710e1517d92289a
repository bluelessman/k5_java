package Chap3;

public class Test21 {
	public static void mai(String[] args) {
		int a = 14;
		int b = 2;
		char op = '+';
		System.out.println(a);
		if(op == '+') {
			System.out.println(a+b);
		}
		else if(op == '-') {
			System.out.println(a-b);
		}
		else if(op == '*') {
			System.out.println(a*b);
		}
		else if(op == '/') {
			System.out.println(a/b);
		}
	}

}
