package Chap10;

import java.util.ArrayList;
import java.util.List;
import java.util.Random;

public class Test03 {
public static void main(String[] args) {
	List<Integer> arr = new ArrayList<>();
	Random rd = new Random();
	for(int i=0;i<100;i++) {
		arr.add(rd.nextInt(101));
	}
	System.out.println(arr);
}
}
