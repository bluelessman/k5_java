package chap2;
class Static{
	 
    public int a = 20;
    static int b = 0;
    
}
 

public class Test1 {

	public static void main(String[] args) {
		
		System.out.println("Hello Java!");
		int t = 1;
		for(int i=0;i<10;i++) {
			System.out.print(t++ + " ");
		}
        int a;
        a = 10;
        Static.b = a;
 
        Static st = new Static();
 
        System.out.println(Static.b++);
        System.out.println(st.b);
        System.out.println(a);
        System.out.print(st.a);
	}

}
