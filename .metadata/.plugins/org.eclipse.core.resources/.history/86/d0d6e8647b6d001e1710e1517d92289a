package Chap3;

public class Test22 {
	public static void main(String[] args) {
		int a = 14;
		int b = 2;
		char op = '-';
		switch(op) {
		case '+':
			System.out.println(a+b);
			break;
		case '-':
			System.out.println(a-b);
			break;
		case '*':
			System.out.println(a*b);
			break;
		case '/':
			System.out.println(a/b);
			break;
		}

	}

}
