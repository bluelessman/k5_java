package Chap10;

import java.util.ArrayList;
import java.util.List;

public class Star {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		print_star(5);
	}
public static void print_star(int n) {
	List<String> star = new ArrayList<>();
	for(int i = 0;i<n;i++) {
		star.add("*");
		star.forEach(s->System.out.print(s));
		System.out.println();
	}
}
}
